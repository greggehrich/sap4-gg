<script type="text/javascript" id="favorite_places_json">
  <%= raw @favorite_places_json %>
</script>

<div class="panel top-margin-sap" style="padding-bottom: 0px; padding-right: 5px" >
  <p style="font-size: 1.2em; display: inline">Search My Saved Places</p>&nbsp;&nbsp;
  <div class="container left-margin-zero-sap">
    <div class="row" style="padding-top: 5px">

	    <div class="map_search col-md-12">
	      <form action="javascript:void(0)" method="GET" class="form-inline map_search_form" data-user-id="1">
	        <div class="form-group col-md-12" style="display: inline">
	          <input type="text" name="search" class="form-control" placeholder="Enter a Location"/>
	        </div>
	        <p style="display: inline"><button type="submit" class="btn btn-default">Search</button></p>
	      </form>
	    </div>
	    <div class="col-md-12">
	      <div id="search_map_canvas" style="height: 100%"></div>
	    </div>

		</div>
	</div>
</div>

	<!--///////////////////////////////////////////////////////////////////////////////////////////////////////////-->

<script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>
<%= javascript_include_tag 'application' %>
