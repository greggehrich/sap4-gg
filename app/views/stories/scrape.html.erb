<%= form_tag( :action => "new" ) do %>
  <br><br><br><br>
  <div class="panel panel-default">
    <div class="panel-body">
      <br>
    <table>
      <tr>
        <td><div style="float: left; margin-left: 1px "<%= button_to("Process URL", nil, class: 'btn btn-sm btn-success') %></div></td>
        <td>&nbsp</td>
        <td><%= text_field_tag(:source_url_pre, nil, autofocus: true, :maxlength => 250, :size => 100) %></td>
        <td><%= hidden_field_tag(:data_entry_begin_time, Time.now) %></td>
      </tr>
    </table>
    <br>
  </div></div>

  <%= debug(params) if Rails.env.development? %>
  <%= debug(flash) if Rails.env.development? %>

<% end %>