<%= form_tag( :action => "new" ) do %>
  <br><br><br><br>
  <div class="panel panel-default">
    <div class="panel-body">

      <div class="col-sm-2 col-md-2 col-lg-1" style="margin-right: auto; margin-top: 10px">
	      <%= button_to "Process URL", class: 'btn btn-sm btn-success' %>
      </div>

      <div class="col-sm-10 col-md-10 col-lg-11" style="margin-right: auto;">
				<%= text_field_tag(:source_url_pre, nil, autofocus: true, class: "form-control") %>
			</div>

			  <!--set timing field-->
		    <%= hidden_field_tag(:data_entry_begin_time, Time.now) %>

    </div>
  </div>

<% end %>